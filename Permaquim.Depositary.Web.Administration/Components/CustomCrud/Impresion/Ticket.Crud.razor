    private List<Depositary.Entities.Tables.Impresion.Ticket> ObtenerRegistrosTicketRestantes(Int64 TipoId, Int64 DepositarioModeloId)
    {
        List<Depositary.Entities.Tables.Impresion.Ticket> resultado = new();

        Depositary.Business.Tables.Impresion.Ticket oTicket = new();
        oTicket.Where.Add(Depositary.Business.Tables.Impresion.Ticket.ColumnEnum.EmpresaId, Depositary.sqlEnum.OperandEnum.NotEqual, EmpresaId);
        oTicket.Where.Add(Depositary.sqlEnum.ConjunctionEnum.AND, Depositary.Business.Tables.Impresion.Ticket.ColumnEnum.TipoId, Depositary.sqlEnum.OperandEnum.Equal, TipoId);
        oTicket.Where.Add(Depositary.sqlEnum.ConjunctionEnum.AND, Depositary.Business.Tables.Impresion.Ticket.ColumnEnum.DepositarioModeloId, Depositary.sqlEnum.OperandEnum.Equal, DepositarioModeloId);

        resultado = oTicket.Items();

        return resultado;
    }

    /// <summary>
    /// Operation depending on Crud mode
    /// </summary>
    private async Task Save()
    {
        estaProcesando = true;
        StateHasChanged();
        string actionInformation = string.Empty;
        NotificationSeverity severityType = NotificationSeverity.Error;
        string summary = string.Empty;
        Depositary.Business.Tables.Impresion.Ticket crud = new();
        Ticket_entity.EmpresaId = EmpresaId;
        foreach (var property in Ticket_entity.GetType().GetProperties())
        {
            switch (property.Name)
            {
                case "FechaModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Ticket_entity.FechaModificacion = null;
                    }
                    else
                    {
                        Ticket_entity.FechaModificacion = DateTime.Now;
                    }
                    break;
                case "FechaCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Ticket_entity.FechaCreacion = DateTime.Now;
                    }
                    break;
                case "UsuarioCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Ticket_entity.UsuarioCreacion = dataUsuario.Id;
                    }
                    break;
                case "UsuarioModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Ticket_entity.UsuarioModificacion = null;
                    }
                    else
                    {
                        Ticket_entity.UsuarioModificacion = dataUsuario.Id;
                    }
                    break;
            }
        }
        switch (crudMode)
        {
            case CrudMode.Add:
                if (EsDefault)
                {
                    crud.BeginTransaction();
                    try
                    {
                        crud.Add(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);

                        //Obtenemos los ID de todas las empresas existentes (salvo esta) e insertamos el registro de ticket en ellas
                        var empresasTicketsRestantes = DirectorioController.ObtenerEmpresasNoDefault(EmpresaId);

                        if (empresasTicketsRestantes.Count > 0)
                        {
                            foreach (var empresa in empresasTicketsRestantes)
                            {
                                Depositary.Business.Tables.Impresion.Ticket oTicket = new();
                                Ticket_entity.EmpresaId = empresa.Id;
                                oTicket.Add(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                            }
                        }

                        crud.EndTransaction(true);
                        summary = "El registro se ha ";
                        actionInformation = "agregado en esta empresa y en las restantes.";
                        severityType = NotificationSeverity.Success;
                    }
                    catch (Exception ex)
                    {
                        crud.EndTransaction(false);
                        AuditController.Log(ex, dataUsuario.Id);
                        summary = "Error al procesar.";
                        actionInformation = ex.Message;
                        severityType = NotificationSeverity.Error;
                    }
                }
                break;
            case CrudMode.Edit:
                //Si se modifica el ticket de la empresa default se arrastra el cambio (solo tipo de ticket y modelo de depositario) al resto de empresas.
                if (EsDefault)
                {
                    try
                    {
                        //Si se cambio el tipo de ticket o el modelo de depositario asociado le pego a todas las empresas
                        if (tipoIdAnteriorTicket != Ticket_entity.TipoId || depositarioModeloIdAnteriorTicket != Ticket_entity.DepositarioModeloId)
                        {
                            crud.BeginTransaction();

                            crud.Update(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);

                            //Obtenemos los ID de todas  las empresas existentes (salvo esta)
                            var empresasTicketsRestantes = ObtenerRegistrosTicketRestantes(tipoIdAnteriorTicket, depositarioModeloIdAnteriorTicket);

                            if (empresasTicketsRestantes.Count > 0)
                            {
                                Depositary.Business.Tables.Impresion.Ticket oTicket = new(crud);
                                foreach (var registroTicket in empresasTicketsRestantes)
                                {
                                    registroTicket.DepositarioModeloId = Ticket_entity.DepositarioModeloId;
                                    registroTicket.TipoId = Ticket_entity.TipoId;
                                    registroTicket.FechaModificacion = DateTime.Now;
                                    registroTicket.UsuarioModificacion = dataUsuario.Id;
                                    registroTicket.Habilitado = Ticket_entity.Habilitado;
                                    oTicket.Update(registroTicket, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                                }
                            }

                            crud.EndTransaction(true);
                            actionInformation = "modificado en esta empresa y en las restantes.";
                        }
                        else
                        {
                            crud.Update(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                            actionInformation = "modificado.";
                        }

                        summary = "El registro se ha ";
                        severityType = NotificationSeverity.Success;
                    }
                    catch (Exception ex)
                    {
                        crud.EndTransaction(false);
                        AuditController.Log(ex, dataUsuario.Id);
                        summary = "Error al procesar.";
                        actionInformation = ex.Message;
                        severityType = NotificationSeverity.Error;
                    }
                }
                else
                {
                    try
                    {
                        crud.Update(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                        summary = "El registro se ha ";
                        actionInformation = "modificado.";
                        severityType = NotificationSeverity.Success;
                    }
                    catch (Exception ex)
                    {
                        AuditController.Log(ex, dataUsuario.Id);
                        summary = "Error al procesar.";
                        actionInformation = ex.Message;
                        severityType = NotificationSeverity.Error;
                    }
                }

                break;
            case CrudMode.Delete:
                try
                {
                    Ticket_entity.Habilitado = false;
                    crud.Update(Ticket_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                    summary = "El registro se ha ";
                    actionInformation = "eliminado.";
                    severityType = NotificationSeverity.Success;
                }
                catch (Exception ex)
                {
                    AuditController.Log(ex, dataUsuario.Id);
                    summary = "Error al procesar.";
                    actionInformation = ex.Message;
                    severityType = NotificationSeverity.Error;
                }
                break;
        }

        // Sets Crud mode to List
        crudMode = CrudMode.List;

        ShowNotification(new NotificationMessage
            {
                Severity = severityType,
                Summary = summary,
                Detail = actionInformation,
                Duration = 4000
            });

        estaProcesando = false;
        StateHasChanged();

        await Task.Run(LoadMainEntityData);
    }
