        }

        // Sets Crud mode to List
        crudMode = CrudMode.List;

        ShowNotification(new NotificationMessage
            {
                Severity = severityType,
                Summary = summary,
                Detail = actionInformation,
                Duration = 4000
            });

        await Task.Run(LoadMainEntityData);
    }

    #region Grid events

    protected async Task expandirMoneda(Depositary.Entities.Tables.Valor.Moneda args)
    {
        rowsMonedasExpandidas.Add(args);
    }

    protected async Task colapsarMoneda(Depositary.Entities.Tables.Valor.Moneda args)
    {
        rowsMonedasExpandidas.RemoveAll(x => x.Id == args.Id);
    }

    void monedaCellRender(DataGridCellRenderEventArgs<Depositary.Entities.Tables.Valor.Moneda> args)
    {
        if (rowsMonedasExpandidas.Where(x => x.Id == args.Data.Id).ToList().Count > 0)
        {
            args.Attributes.Add("class", "row-highlight");
        }
        else
        {
            if (!args.Data.Habilitado)
                args.Attributes.Add("class", "row-highlight-disabled");
            else
                args.Attributes.Add("class", "rz-datatable-even");
        }
    }

