    /// <summary>
    /// Operation depending on Crud mode
    /// </summary>
    private async Task Save()
    {
        string actionInformation = string.Empty;
        NotificationSeverity severityType = NotificationSeverity.Error;
        string summary = string.Empty;
        Depositary.Business.Tables.Directorio.Empresa crud = new();
        Empresa_entity.GrupoId = ParametroGrupoId;
        foreach (var property in Empresa_entity.GetType().GetProperties())
        {
            switch (property.Name)
            {
                case "FechaModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Empresa_entity.FechaModificacion = null;
                    }
                    else
                    {
                        Empresa_entity.FechaModificacion = DateTime.Now;
                    }
                    break;
                case "FechaCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Empresa_entity.FechaCreacion = DateTime.Now;
                    }
                    break;
                case "UsuarioCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Empresa_entity.UsuarioCreacion = dataUsuario.Id;
                    }
                    break;
                case "UsuarioModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Empresa_entity.UsuarioModificacion = null;
                    }
                    else
                    {
                        Empresa_entity.UsuarioModificacion = dataUsuario.Id;
                    }
                    break;
            }
        }
        if (crudMode == CrudMode.Add || crudMode == CrudMode.Edit)
        {
            empresaDefault = ObtenerEmpresaDefault();

            if (Empresa_entity.EsDefault && empresaDefault != null)
            {
                summary = "Error al procesar.";
                actionInformation = "No se puede guardar/editar esta empresa como default debido a que ya existe una.";
                severityType = NotificationSeverity.Error;
            }
            else
            {
                switch (crudMode)
                {
                    case CrudMode.Add:
                        try
                        {
                            crud.Add(Empresa_entity);

                            //Si se agrego una empresa no default y ademas existia una empresa default entonces le cargamos las configuraciones de la empresa default
                            if (!Empresa_entity.EsDefault && empresaDefault != null)
                            {
                                if (!AplicacionController.GenerarRegistrosConfiguracionEmpresa(Empresa_entity.Id))
                                {
                                    summary = "Error al procesar.";
                                    actionInformation = "Se pudo generar la empresa pero no se pudieron insertar las configuraciones de la empresa default";
                                    severityType = NotificationSeverity.Warning;
                                }
                                else
                                {
                                    summary = "Exito.";
                                    actionInformation = "Se genero la empresa y se cargaron las configuraciones de la empresa default.";
                                    severityType = NotificationSeverity.Success;
                                }
                            }
                            else
                            {
                                summary = "El registro se ha ";
                                actionInformation = "agregado.";
                                severityType = NotificationSeverity.Success;
                            }
                        }
                        catch (Exception ex)
                        {
                            summary = "Error al procesar.";
                            actionInformation = ex.Message;
                            severityType = NotificationSeverity.Error;
                        }
                        break;
                    case CrudMode.Edit:
                        try
                        {
                            crud.Update(Empresa_entity);
                            summary = "El registro se ha ";
                            actionInformation = "modificado.";
                            severityType = NotificationSeverity.Success;
                        }
                        catch (Exception ex)
                        {
                            summary = "Error al procesar.";
                            actionInformation = ex.Message;
                            severityType = NotificationSeverity.Error;
                        }
                        break;
                }
            }
        }
        else
        {
            try
            {
                Empresa_entity.Habilitado = false;
                crud.Update(Empresa_entity);
                summary = "El registro se ha ";
                actionInformation = "eliminado.";
                severityType = NotificationSeverity.Success;
            }
            catch (Exception ex)
            {
                summary = "Error al procesar.";
                actionInformation = ex.Message;
                severityType = NotificationSeverity.Error;
            }
        }

        // Sets Crud mode to List
        crudMode = CrudMode.List;
        rowsEmpresasExpandidas.Clear();

        ShowNotification(new NotificationMessage
            {
                Severity = severityType,
                Summary = summary,
                Detail = actionInformation,
                Duration = 4000
            });

        await Task.Run(LoadMainEntityData);
    }
