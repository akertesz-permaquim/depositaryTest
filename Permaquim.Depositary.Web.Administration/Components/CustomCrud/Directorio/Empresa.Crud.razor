    void empresaCellRender(DataGridCellRenderEventArgs<Depositary.Entities.Tables.Directorio.Empresa> args)
    {
        if (args.Column.Property == "GrupoId")
        {
            args.Attributes.Add("style", colorCeldaCampoDependiente);
        }
        else
        {
            if (rowsEmpresasExpandidas.Where(x => x.Id == args.Data.Id).ToList().Count > 0)
            {
                args.Attributes.Add("class", "row-highlight");
            }
            else
            {
                if (!args.Data.Habilitado)
                    args.Attributes.Add("class", "row-highlight-disabled");
                else
                    args.Attributes.Add("class", "rz-datatable-even");
            }
        }
    }

    protected async Task expandirEmpresa(Depositary.Entities.Tables.Directorio.Empresa args)
    {
        rowsEmpresasExpandidas.Add(args);
    }

    protected async Task colapsarEmpresa(Depositary.Entities.Tables.Directorio.Empresa args)
    {
        rowsEmpresasExpandidas.RemoveAll(x => x.Id == args.Id);
    }

