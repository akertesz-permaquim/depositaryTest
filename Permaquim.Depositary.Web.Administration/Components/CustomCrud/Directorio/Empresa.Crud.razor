                        }
                        else
                        {
                            Empresa_entity.UsuarioModificacion = dataUsuario.Id;
                        }
                        break;
                }
            }
            if (crudMode == CrudMode.Add || crudMode == CrudMode.Edit)
            {
                switch (crudMode)
                {
                    case CrudMode.Add:
                        try
                        {

                            //Si se agrego una empresa no default y ademas existia una empresa default
                            //entonces le cargamos las configuraciones y los tickets de la empresa default.
                            if (!Empresa_entity.EsDefault && empresaDefault != null)
                            {
                                crud.Add(Empresa_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                                string mensajeValidacion = "";
                                if (!AplicacionController.GenerarRegistrosConfiguracionEmpresa(Empresa_entity.Id))
                                {
                                    mensajeValidacion += "No se pudieron insertar las configuraciones de la empresa default";
                                    severityType = NotificationSeverity.Warning;
                                }

                                if (!ImpresionController.GenerarRegistrosTicketEmpresa(Empresa_entity.Id))
                                {
                                    mensajeValidacion += "No se pudieron insertar los tickets de la empresa default";
                                    severityType = NotificationSeverity.Warning;
                                }

                                else
                                {
                                    summary = "Exito.";
                                    actionInformation = "Se genero la empresa y se cargaron las configuraciones y los tickets de la empresa default.";
                                    severityType = NotificationSeverity.Success;
                                }
                            }
                            else
                            {
                                //Si se agrego una empresa default y ya existia una le quitamos la marca a la anterior
                                if (empresaDefault != null && Empresa_entity.EsDefault)
                                {
                                    Depositary.Business.Tables.Directorio.Empresa controllerEmpresaDefault = new(crud);

                                    try
                                    {
                                        crud.BeginTransaction();

                                        empresaDefault.EsDefault = false;
                                        controllerEmpresaDefault.Update(empresaDefault);

                                        crud.Add(Empresa_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);

                                        crud.EndTransaction(true);

                                    }
                                    catch (Exception exception)
                                    {
                                        crud.EndTransaction(false);
                                        AuditController.Log(exception, dataUsuario.Id);
                                        summary = "Error al procesar.";
                                        actionInformation = exception.Message;
                                        severityType = NotificationSeverity.Error;
                                    }
                                }
                                else
                                {
                                    crud.Add(Empresa_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);
                                }

                                summary = "El registro se ha ";
                                actionInformation = "agregado.";
                                severityType = NotificationSeverity.Success;
                            }
                        }
                        catch (Exception ex)
                        {

                            AuditController.Log(ex, dataUsuario.Id);
                            summary = "Error al procesar.";
                            actionInformation = ex.Message;
                            severityType = NotificationSeverity.Error;
                        }
                        break;
                    case CrudMode.Edit:
                        try
                        {
                            if (empresaDefault != null && Empresa_entity.EsDefault)
                            {
                                Depositary.Business.Tables.Directorio.Empresa controllerEmpresaDefault = new(crud);

                                try
                                {
                                    crud.BeginTransaction();

                                    empresaDefault.EsDefault = false;
                                    controllerEmpresaDefault.Update(empresaDefault);

                                    crud.Update(Empresa_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);

                                    crud.EndTransaction(true);

                                }
                                catch (Exception exception)
                                {
                                    crud.EndTransaction(false);
                                    AuditController.Log(exception, dataUsuario.Id);
                                    summary = "Error al procesar.";
                                    actionInformation = exception.Message;
                                    severityType = NotificationSeverity.Error;
                                }
                            }
                            else
                                crud.Update(Empresa_entity, (long)SeguridadEntities.Aplicacion.AdministradorWeb);

                            summary = "El registro se ha ";
                            actionInformation = "modificado.";
                            severityType = NotificationSeverity.Success;
                        }
                        catch (Exception ex)
                        {
                            AuditController.Log(ex, dataUsuario.Id);
                            summary = "Error al procesar.";
                            actionInformation = ex.Message;
                            severityType = NotificationSeverity.Error;
                        }
                        break;
                }
            }
            else
            {
                try
                {
                    Empresa_entity.Habilitado = false;
                    crud.Update(Empresa_entity);
                    summary = "El registro se ha ";
                    actionInformation = "eliminado.";
                    severityType = NotificationSeverity.Success;
                }
                catch (Exception ex)
                {
                    AuditController.Log(ex, dataUsuario.Id);
                    summary = "Error al procesar.";
                    actionInformation = ex.Message;
                    severityType = NotificationSeverity.Error;
                }
            }

            // Sets Crud mode to List
            crudMode = CrudMode.List;
            rowsEmpresasExpandidas.Clear();

            ShowNotification(new NotificationMessage
                {
                    Severity = severityType,
                    Summary = summary,
                    Detail = actionInformation,
                    Duration = 4000
                });

            await Task.Run(LoadMainEntityData);
        }

    }
    /// <summary>
    /// Crud mode cancellation and default mode
    /// </summary>
    private void Cancel()
    {
        crudMode = CrudMode.List;
        LoadMainEntityData();
    }
