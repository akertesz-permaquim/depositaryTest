@page "/VisualizacionPerfilCustomCrud"
@inject NotificationService NotificationService
@inject Blazored.SessionStorage.ISessionStorageService sessionStorage
@inject NavigationManager NavManager

@if (Perfil_entities == null)
{
    <RadzenProgressBar Value="100" ShowValue="false" Mode="ProgressBarMode.Indeterminate" />
}
else
{
    switch (crudMode)
    {
        case CrudMode.Add:
            <RadzenBadge BadgeStyle="BadgeStyle.Secondary">
                <ChildContent>
                    <h4 style="color:white;">@dataLenguaje.Where(x => x.Clave == "ABM_A_TITLE" || x.Clave=="GENERIC").FirstOrDefault().Texto  @dataLenguaje.Where(x => x.Clave == "PERFIL" || x.Clave=="GENERIC").FirstOrDefault().Texto </h4>
                </ChildContent>
            </RadzenBadge>
            break;
        case CrudMode.Delete:
            <RadzenBadge BadgeStyle="BadgeStyle.Warning">
                <ChildContent>
                    <h4 style="color:white;">@dataLenguaje.Where(x => x.Clave == "ABM_D_TITLE" || x.Clave=="GENERIC").FirstOrDefault().Texto  @dataLenguaje.Where(x => x.Clave == "PERFIL" || x.Clave=="GENERIC").FirstOrDefault().Texto? </h4>
                </ChildContent>
            </RadzenBadge>
            break;
        case CrudMode.Edit:
            <RadzenBadge BadgeStyle="BadgeStyle.Secondary">
                <ChildContent>
                    <h4 style="color:white;">@dataLenguaje.Where(x => x.Clave == "ABM_M_TITLE" || x.Clave=="GENERIC").FirstOrDefault().Texto  @dataLenguaje.Where(x => x.Clave == "PERFIL" || x.Clave=="GENERIC").FirstOrDefault().Texto </h4>
                </ChildContent>
            </RadzenBadge>
            break;
        case CrudMode.List:
            <RadzenBadge BadgeStyle="BadgeStyle.Secondary">
                <ChildContent>
                    <div>
                        <h4 style="color:white;">@dataLenguaje.Where(x => x.Clave == "ABM_L_TITLE" || x.Clave=="GENERIC").FirstOrDefault().Texto  @dataLenguaje.Where(x => x.Clave == "PERFIL" || x.Clave=="GENERIC").FirstOrDefault().Texto </h4>
                    </div>
                </ChildContent>
            </RadzenBadge>
            break;
    }
    <hr>
    if (crudMode == CrudMode.List)
    {
        <RadzenDataGrid AllowFiltering="true" RowExpand="@expandirPerfil" RowRender="@perfilRowRender" RowCollapse="@colapsarPerfil" AllowColumnResize="true" FilterMode="FilterMode.Advanced" AllowSorting="true" PageSize="10" AllowPaging="true" PagerHorizontalAlign="HorizontalAlign.Left" ShowPagingSummary="true"
                Data="@Perfil_entities" TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" ColumnWidth="300px" LogicalFilterOperator="LogicalFilterOperator.Or">
            <EmptyTemplate>
                <p style="color: lightgrey; font-size: 24px; text-align: center; margin: 2rem;">No existen registros.</p>
            </EmptyTemplate>
            <Columns>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Filterable="false" Sortable="false" TextAlign="TextAlign.Center" Width="80px">
                    <HeaderTemplate>
                        <RadzenButton Icon="add_circle_outline" ButtonStyle="ButtonStyle.Success" Text=@dataLenguaje.Where(x => x.Clave == "NEW_BUTTON" || x.Clave == "GENERIC").FirstOrDefault().Texto Click="Add">
                        </RadzenButton>
                    </HeaderTemplate>
                    <Template Context="element">
                        <RadzenButton Icon="edit" ButtonStyle="ButtonStyle.Info" Class="m-1" Click="@(args => Edit(element,false))" @onclick:stopPropagation="true">
                        </RadzenButton>
                        <RadzenButton Icon="delete" ButtonStyle="ButtonStyle.Danger" Class="m-1" Click="@(args => Edit(element,true))" @onclick:stopPropagation="true">
                        </RadzenButton>
                    </Template>
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="Id" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "ID" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center" />
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="Nombre" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "NOMBRE" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="Descripcion" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "DESCRIPCION" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Title=@dataLenguaje.Where(x => x.Clave == "PERFILTIPOID" || x.Clave=="GENERIC").FirstOrDefault().Texto Property="PerfilTipoId" Width="100px">
                    <Template>
                        @if (PerfilTipo_entities.FirstOrDefault(c => c.Id == @context.PerfilTipoId) != null)
                        {
                            <RadzenLabel Text="@PerfilTipo_entities.FirstOrDefault( c => c.Id == @context.PerfilTipoId).Nombre" Component="PerfilTipoId" style="width: 100%">
                            </RadzenLabel>
                        }
                        else
                        {
                            <RadzenLabel Text="No data available" Component="PerfilTipoId" style="width: 100%">
                            </RadzenLabel>
                        }
                    </Template>
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="Habilitado" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "HABILITADO" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                    <Template Context="data">
                        <RadzenCheckBox TValue="bool" Value=@data.Habilitado Disabled="true">
                        </RadzenCheckBox>
                    </Template>
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="UsuarioCreacion" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "USUARIOCREACION" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="FechaCreacion" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "FECHACREACION" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="UsuarioModificacion" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "USUARIOMODIFICACION" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Property="FechaModificacion" Filterable="false" Title=@dataLenguaje.Where(x => x.Clave == "FECHAMODIFICACION" || x.Clave=="GENERIC").FirstOrDefault().Texto Frozen="false" Width="50px" TextAlign="TextAlign.Center">
                </RadzenDataGridColumn>
            </Columns>
            <Template Context="perfil">
                <PerfilItem_Crud PerfilId="@(perfil.Id)"></PerfilItem_Crud>
            </Template>
        </RadzenDataGrid>
        <hr>
    }

    if (crudMode == CrudMode.Add || crudMode == CrudMode.Edit || crudMode == CrudMode.Delete)
    {
        <RadzenTemplateForm TItem="DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil" Data="@Perfil_entity" @bind-Value="Perfil_entity" Submit="Save" Visible="@(Perfil_entity != null)">
            <ChildContent>
                <div style="margin-bottom: 1rem" class="row">
                    <div class="col-md-3">
                        <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "ID" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="Id" style="width: 100%">
                        </RadzenLabel>
                    </div>
                    <div class="col-md-9">
                        <RadzenNumeric TValue="long" style="display: block; width: 100%" @bind-Value="@(Perfil_entity.Id)" Name="Id" Disabled="true">
                        </RadzenNumeric>
                    </div>
                </div>
                <div style="margin-bottom: 1rem" class="row">
                    <div class="col-md-3">
                        <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "NOMBRE" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="Nombre" style="width: 100%">
                        </RadzenLabel>
                    </div>
                    <div class="col-md-9">
                        <RadzenTextBox style="display: block" @bind-Value="@(Perfil_entity.Nombre)" Name="Nombre" Disabled="@(crudMode==CrudMode.Delete)">
                        </RadzenTextBox>
                        @if (@crudMode != CrudMode.Delete)
                        {
                            <RadzenRequiredValidator Component="Nombre" Text="Nombre is required" Style="position: absolute">
                            </RadzenRequiredValidator>
                        }
                    </div>
                </div>
                <div style="margin-bottom: 1rem" class="row">
                    <div class="col-md-3">
                        <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "DESCRIPCION" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="Descripcion" style="width: 100%">
                        </RadzenLabel>
                    </div>
                    <div class="col-md-9">
                        <RadzenTextBox style="display: block" @bind-Value="@(Perfil_entity.Descripcion)" Name="Descripcion" Disabled="@(crudMode==CrudMode.Delete)">
                        </RadzenTextBox>
                        @if (@crudMode != CrudMode.Delete)
                        {
                            <RadzenRequiredValidator Component="Descripcion" Text="Descripcion is required" Style="position: absolute">
                            </RadzenRequiredValidator>
                        }
                    </div>
                </div>
                <div style="margin-bottom: 1rem" class="row">
                    <div class="col-md-3">
                        <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "PERFILTIPOID" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="PerfilTipoId" style="width: 100%">
                        </RadzenLabel>
                    </div>
                    <div class="col-md-9">
                        @if ((@PerfilTipo_entities.Count > 0 && @PerfilTipo_entities.FirstOrDefault(c => c.Id == @Perfil_entity.PerfilTipoId) != null) || crudMode != CrudMode.Delete)
                        {
                            <RadzenDropDown AllowClear="true" TValue="Int64" Class="w-100" @bind-Value="@(Perfil_entity.PerfilTipoId)"
                                Data=@PerfilTipo_entities TextProperty="Nombre" ValueProperty="Id"
                                Change=@(args => SelectPerfilTipoValueChanged(args))
                                Disabled="@(crudMode==CrudMode.Delete)" />
                        }
                        else
                        {
                            <RadzenDropDown AllowClear="true" TValue="Int64" Class="w-100"
                                Data=@PerfilTipo_entities TextProperty="Nombre" ValueProperty="Id"
                                Change=@(args => SelectPerfilTipoValueChanged(args))
                                Disabled="@(crudMode==CrudMode.Delete)" />
                        }
                    </div>
                </div>
                <div style="margin-bottom: 1rem" class="row">
                    <div class="col-md-3">
                        <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "HABILITADO" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="Habilitado" Style="margin-left: 0px; vertical-align: middle;" />
                    </div>
                    <div class="col-md-9">
                        <RadzenCheckBox @bind-Value=@Perfil_entity.Habilitado Name="HabilitadoCheckBox" TValue="bool" Change=@(args => HabilitadoChange(args, @Perfil_entity)) Disabled="@(crudMode == CrudMode.Delete)" />
                    </div>
                </div>
                @if (crudMode != CrudMode.Add)
                {
                    <div style="margin-bottom: 1rem" class="row">
                        <div class="col-md-3">
                            <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "USUARIOCREACION" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="UsuarioCreacion" style="width: 100%">
                            </RadzenLabel>
                        </div>
                        <div class="col-md-9">
                            <RadzenNumeric TValue="long" style="display: block; width: 100%" @bind-Value="@(Perfil_entity.UsuarioCreacion)" Name="UsuarioCreacion" Disabled="@(crudMode==CrudMode.Delete)">
                            </RadzenNumeric>
                        </div>
                    </div>
                }
                @if (crudMode != CrudMode.Add)
                {
                    <div style="margin-bottom: 1rem" class="row">
                        <div class="col-md-3">
                            <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "FECHACREACION" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="FechaCreacion" style="width: 100%">
                            </RadzenLabel>
                        </div>
                        <div class="col-md-9">
                            <RadzenDatePicker ShowTime="true" ShowSeconds="true" style="width: 25%" @bind-Value="@(Perfil_entity.FechaCreacion)" Name="FechaCreacion" Disabled="@(crudMode==CrudMode.Delete)">
                            </RadzenDatePicker>
                        </div>
                    </div>
                }
                @if (crudMode != CrudMode.Add)
                {
                    <div style="margin-bottom: 1rem" class="row">
                        <div class="col-md-3">
                            <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "USUARIOMODIFICACION" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="UsuarioModificacion" style="width: 100%">
                            </RadzenLabel>
                        </div>
                        <div class="col-md-9">
                            <RadzenNumeric TValue="long?" style="display: block; width: 100%" @bind-Value="@(Perfil_entity.UsuarioModificacion)" Name="UsuarioModificacion" Disabled="@(crudMode==CrudMode.Delete)">
                            </RadzenNumeric>
                        </div>
                    </div>
                }
                @if (crudMode != CrudMode.Add)
                {
                    <div style="margin-bottom: 1rem" class="row">
                        <div class="col-md-3">
                            <RadzenLabel Text=@dataLenguaje.Where(x => x.Clave == "FECHAMODIFICACION" || x.Clave == "GENERIC").FirstOrDefault().Texto Component="FechaModificacion" style="width: 100%">
                            </RadzenLabel>
                        </div>
                        <div class="col-md-9">
                            <RadzenDatePicker ShowTime="true" ShowSeconds="true" style="width: 25%" @bind-Value="@(Perfil_entity.FechaModificacion)" Name="FechaModificacion" Disabled="@(crudMode==CrudMode.Delete)">
                            </RadzenDatePicker>
                        </div>
                    </div>
                }
                <div class="row">
                    <div class="col offset-sm-3">
                        <RadzenButton ButtonType="ButtonType.Submit" Icon="save" Text=@dataLenguaje.Where(x => x.Clave == "ACCEPT_BUTTON" || x.Clave=="GENERIC").FirstOrDefault().Texto ButtonStyle="ButtonStyle.Primary">
                        </RadzenButton>
                        <RadzenButton ButtonStyle="ButtonStyle.Secondary" Icon="save" style="margin-left: 1rem" Text=@dataLenguaje.Where(x => x.Clave == "CANCEL_BUTTON" || x.Clave=="GENERIC").FirstOrDefault().Texto Click="Cancel">
                        </RadzenButton>
                    </div>
                </div>
            </ChildContent>
        </RadzenTemplateForm>
        <hr>
    }

    <RadzenNotification />
}

@code {
    /// <summary>
    /// Crud Mode Enum
    /// </summary>
    private enum CrudMode
    {
        List,
        Add,
        Edit,
        Delete
    }
    /// <summary>
    /// Variables to handle main entity
    /// </summary>
    private DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil Perfil_entity = new();
    private List<DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil> Perfil_entities = null;

    private List<DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil> rowsPerfilesExpandidos = new List<DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil>();

    /// <summary>
    /// Variables to multilanguaje
    /// </summary>
    private List<DepositarioAdminWeb.Entities.Procedures.Regionalizacion.ObtenerTextosLenguaje.Resultado> dataLenguaje;

    /// <summary>
    /// Variables to handle Relation between Perfil.PerfilTipoId and PerfilTipo.Id
    /// </summary>
    private DepositarioAdminWeb.Entities.Tables.Visualizacion.PerfilTipo PerfilTipo_entity = new();
    private List<DepositarioAdminWeb.Entities.Tables.Visualizacion.PerfilTipo>? PerfilTipo_entities;

    /// <summary>
    ///  Page crud mode {List,Add,Edit,Delete}
    /// </summary>
    private CrudMode crudMode = CrudMode.List;

    /// <summary>
    ///  Variable to store the user id
    /// </summary>
    private Int64? userId = null;

    protected override async Task OnInitializedAsync()
    {
        dataLenguaje = await sessionStorage.GetItemAsync<List<DepositarioAdminWeb.Entities.Procedures.Regionalizacion.ObtenerTextosLenguaje.Resultado>>("DataLenguaje");
        await Task.Run(LoadMainEntityData);
        await Task.Run(LoadTypesData);
        userId = await sessionStorage.GetItemAsync<Int64?>("Id");
        if (userId == null)
            NavManager.NavigateTo("login", true);
    }
    #region Loading entities data
    /// <summary>
    /// Loads main entity data
    /// </summary>
    private void LoadMainEntityData()
    {
        DepositarioAdminWeb.Business.Tables.Visualizacion.Perfil entity = new();
        Perfil_entities = entity.Items();
    }
    /// <summary>
    /// Loads Type Tables data
    /// </summary>
    private void LoadTypesData()
    {
        DepositarioAdminWeb.Business.Tables.Visualizacion.PerfilTipo PerfilTipo_local = new();
        PerfilTipo_entities = PerfilTipo_local.Items();

    }

    #endregion

    /// <summary>
    /// Initializations for adding a new entity
    /// </summary>
    private void Add()
    {
        //Main entity initialization
        Perfil_entity = new();
        //Relations entities initialization
        PerfilTipo_entity = new();
        crudMode = CrudMode.Add;
    }
    /// <summary>
    /// <summary>
    /// Initializations for editing an entity
    /// </summary>
    private void Edit(DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil item, bool delete)
    {
        Perfil_entity = item;
        PerfilTipo_entity = new()
            {
                Id = PerfilTipo_entities.FirstOrDefault(c => c.Id == Perfil_entity.PerfilTipoId).Id,
                Nombre = PerfilTipo_entities.FirstOrDefault(c => c.Id == Perfil_entity.PerfilTipoId).Nombre
            };
        if (delete)
            crudMode = CrudMode.Delete;
        else
            crudMode = CrudMode.Edit;
    }
    /// <summary>
    /// Operation depending on Crud mode
    /// </summary>
    private async Task Save()
    {
        string actionInformation = string.Empty;
        NotificationSeverity severityType = NotificationSeverity.Error;
        string summary = string.Empty;
        DepositarioAdminWeb.Business.Tables.Visualizacion.Perfil crud = new();
        foreach (var property in Perfil_entity.GetType().GetProperties())
        {
            switch (property.Name)
            {
                case "FechaModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Perfil_entity.FechaModificacion = null;
                    }
                    else
                    {
                        Perfil_entity.FechaModificacion = DateTime.Now;
                    }
                    break;
                case "FechaCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Perfil_entity.FechaCreacion = DateTime.Now;
                    }
                    break;
                case "UsuarioCreacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Perfil_entity.UsuarioCreacion = userId.Value;
                    }
                    break;
                case "UsuarioModificacion":
                    if (crudMode == CrudMode.Add)
                    {
                        Perfil_entity.UsuarioModificacion = null;
                    }
                    else
                    {
                        Perfil_entity.UsuarioModificacion = userId.Value;
                    }
                    break;
            }
        }
        switch (crudMode)
        {
            case CrudMode.Add:
                try
                {
                    crud.Add(Perfil_entity);
                    summary = "El registro se ha ";
                    actionInformation = "agregado.";
                    severityType = NotificationSeverity.Success;
                }
                catch (Exception ex)
                {
                    summary = "Error al procesar.";
                    actionInformation = ex.Message;
                    severityType = NotificationSeverity.Error;
                }
                break;
            case CrudMode.Edit:
                try
                {
                    crud.Update(Perfil_entity);
                    summary = "El registro se ha ";
                    actionInformation = "modificado.";
                    severityType = NotificationSeverity.Success;
                }
                catch (Exception ex)
                {
                    summary = "Error al procesar.";
                    actionInformation = ex.Message;
                    severityType = NotificationSeverity.Error;
                }
                break;
            case CrudMode.Delete:
                try
                {
                    Perfil_entity.Habilitado = false;
                    crud.Update(Perfil_entity);
                    summary = "El registro se ha ";
                    actionInformation = "eliminado.";
                    severityType = NotificationSeverity.Success;
                }
                catch (Exception ex)
                {
                    summary = "Error al procesar.";
                    actionInformation = ex.Message;
                    severityType = NotificationSeverity.Error;
                }
                break;
        }

        // Sets Crud mode to List
        crudMode = CrudMode.List;

        ShowNotification(new NotificationMessage
            {
                Severity = severityType,
                Summary = summary,
                Detail = actionInformation,
                Duration = 4000
            });

        await Task.Run(LoadMainEntityData);
    }

    #region Grid events

    protected async Task expandirPerfil(DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil args)
    {
        rowsPerfilesExpandidos.Add(args);
    }

    protected async Task colapsarPerfil(DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil args)
    {
        rowsPerfilesExpandidos.RemoveAll(x => x.Id == args.Id);
    }

    void perfilRowRender(RowRenderEventArgs<DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil> args)
    {
        if (rowsPerfilesExpandidos.Where(x => x.Id == args.Data.Id).ToList().Count > 0)
        {
            args.Attributes.Add("class", "row-highlight");
        }
        else
        {
            args.Attributes.Add("class", "rz-datatable-even");
        }
    }

    #endregion

    /// <summary>
    /// Crud mode cancellation and default mode
    /// </summary>
    private void Cancel()
    {
        crudMode = CrudMode.List;
        LoadMainEntityData();
    }
    /// <summary>
    /// Sets detail panel title - TODO: Implement Multilanguage
    /// </summary>
    /// <returns></returns>
    private string GetDialogTitle()
    {
        string retValue = string.Empty;
        switch (crudMode)
        {
            case CrudMode.Add:
                retValue = "Nuevo...";
                break;
            case CrudMode.Edit:
                retValue = "Editar...";
                break;
            case CrudMode.Delete:
                retValue = "Borrar...";
                break;
        }
        return retValue;
    }
    #region Notifications
    private void ShowNotification(NotificationMessage message)
    {
        NotificationService.Notify(message);

    }

    #endregion
    #region Selection events
    /// <summary>
    /// PerfilTipo value changed
    /// </summary>
    /// <param name="item"></param>
    private void SelectPerfilTipoValueChanged(object item)
    {
    }

    /// <summary>
    /// Entity is binded, however it sets anyway.
    /// </summary>
    /// <returns></returns>
    void HabilitadoChange(bool? value, DepositarioAdminWeb.Entities.Tables.Visualizacion.Perfil item)
    {
        Perfil_entity = item;
    }
    #endregion

    #region Export

    #endregion

}
