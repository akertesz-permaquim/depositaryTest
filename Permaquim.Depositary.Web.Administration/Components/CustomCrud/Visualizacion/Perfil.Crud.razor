    /// Variables to handle Relation between Perfil.PerfilTipoId and PerfilTipo.Id
    /// </summary>
    private Depositary.Entities.Tables.Visualizacion.PerfilTipo PerfilTipo_entity = new();
    private List<Depositary.Entities.Tables.Visualizacion.PerfilTipo>? PerfilTipo_entities;
